{
  "name": "lacona-translate",
  "version": "0.2.0",
  "description": "Lacona Command for translating phrases via Google Translate",
  "main": "build/extensions.js",
  "lacona": {
    "extensions": "build/extensions.js"
  },
  "scripts": {
    "build": "mkdir -p build; browserify src/extensions.jsx -o build/extensions.js -x lacona-phrases -x elliptical -x lacona-api --standalone extensions --extension=jsx",
    "clean": "rimraf build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-translate.git"
  },
  "keywords": [
    "lacona",
    "translate"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-translate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-translate",
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "rimraf": "^2.5.2",
    "lodash": "^4.13.1"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
