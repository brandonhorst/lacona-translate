{
  "name": "lacona-translate",
  "version": "0.1.0",
  "description": "Lacona Command for translating phrases via Google Translate",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p lib; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-translate.git"
  },
  "keywords": [
    "lacona",
    "translate"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-translate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-translate",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc8",
    "elliptical-string": "^1.0.0-rc2",
    "elliptical-url": "^1.0.0-rc2",
    "lacona-api": "0.0.1",
    "lacona-command": "^0.1.0",
    "lodash": "^4.6.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
